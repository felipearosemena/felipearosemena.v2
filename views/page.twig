{% extends "layouts/layout-base.twig" %}

{% block content %}

    <div class="container container--small">
      
        <article class="post-type-{{post.post_type}}" id="post-{{post.ID}}">
            
            <header class="entry-header ">
                <h1 class="entry-title">{{ post.title }}</h1>
            </header>

            {{post.content}}
            
        </article>
    
    </div>

{% endblock %}